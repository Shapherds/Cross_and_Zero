<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    android:foreground="@drawable/field"
    tools:context=".MainUserGameActivity">

    <ImageView
        android:id="@+id/im1"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_margin="16dp"
        android:onClick="set"
        app:layout_constraintEnd_toStartOf="@+id/im2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/im4"
        tools:layout_editor_absoluteY="55dp" />

    <ImageView
        android:id="@+id/im2"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_marginBottom="11dp"
        android:layout_margin="16dp"
        android:onClick="set"
        app:layout_constraintBottom_toTopOf="@+id/im5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/im3"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_margin="16dp"
        android:onClick="set"
        app:layout_constraintStart_toEndOf="@+id/im2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/im6" />

    <ImageView
        android:id="@+id/im4"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_margin="16dp"
        android:onClick="set"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/im5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/im5"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_margin="16dp"
        android:onClick="set"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/im6"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_margin="16dp"
        android:onClick="set"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/im5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/im7"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_marginTop="16dp"
        android:onClick="set"
        app:layout_constraintEnd_toStartOf="@id/im8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/im4" />
    <ImageView
        android:id="@+id/im8"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_marginTop="16dp"
        android:onClick="set"
        app:layout_constraintEnd_toStartOf="@id/im9"
        app:layout_constraintStart_toEndOf="@id/im7"
        app:layout_constraintTop_toBottomOf="@id/im5"/>
    <ImageView
        android:id="@+id/im9"
        android:layout_width="140dp"
        android:layout_height="200dp"
        android:layout_marginTop="16dp"
        android:onClick="set"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/im8"
        app:layout_constraintTop_toBottomOf="@id/im6"/>
</androidx.constraintlayout.widget.ConstraintLayout>